          "_customHTML": "<section data-bs-version=\"5.1\" class=\"contacts01 politicsm4_contacts01\" group=\"Contacts\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n\t<mbr-parameters>\n\t\t\n\t\t<header>Size</header>\n\t\t<input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\">\n\t\t<input type=\"range\" title=\"Icons Size\" name=\"iconsSize\" min=\"1\" max=\"9\" step=\"1\" value=\"7\" condition=\"showIcons\">\n\t\t<select title=\"Cards\" name=\"cardsAmount\">\n\t\t\t<option value=\"1\">1</option>\n\t\t\t<option value=\"2\">2</option>\n\t\t\t<option value=\"3\" selected>3</option>\n\t\t</select>\n\t\t<header>Show / Hide</header>\n\t\t<input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n\t\t<input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n\t\t<input type=\"checkbox\" title=\"Icons\" name=\"showIcons\" checked>\n\t\t<header>Colors</header>\n\t\t<input type=\"color\" title=\"Line\" name=\"lineColor\" value=\"#000000\" selected condition=\"showTitle\">\n\t\t<input type=\"color\" title=\"Second Line\" name=\"lineColor2\" value=\"#00775c\" selected condition=\"showTitle\">\n\t\t<input type=\"color\" title=\"Cards Bg\" name=\"cardsBg\" value=\"#00775c\">\n\t\t<input type=\"color\" title=\"Second Cards Bg\" name=\"cardsBg1\" value=\"#131313\">\n\t\t<input type=\"color\" title=\"Icons\" name=\"iconsColor\" value=\"#00775c\" selected condition=\"showIcons\">\n\t\t<input type=\"color\" title=\"Second Icon\" name=\"iconsColor2\" value=\"#f9b03e\" selected condition=\"showIcons\">\n\t\t<header>Background</header>\n\t\t<fieldset type=\"background\" name=\"bg\" parallax>\n\t\t\t<input type=\"image\" title=\"Image\" value=\"../_images/background2.jpg\" parallax>\n\t\t\t<input type=\"color\" title=\"Color\" valu